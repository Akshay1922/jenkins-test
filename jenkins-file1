                echo 'This stage add after git webhook added. 3rd'
            }
        }
        stage('Test') {
            steps {
                echo 'This is Test Stage.'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is Deploy Stage.'
            }
        }
    }
}
