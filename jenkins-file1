pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'This is Build Stage.'
                echo 'This stage add after git webhook added.'
                echo '2 pipeline job need to be run.'
            }
        }
        stage('Test') {
            steps {
                echo 'This is Test Stage.'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is Deploy Stage.'
            }
        }
    }
}
